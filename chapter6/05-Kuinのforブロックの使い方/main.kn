{ Kuinのforブロックの使い方 }

func main()
    ; 増減値は省略すると1になります
    ; for(初期値, 終値)
    for(1, 5)
        ; 初期値が+1されましたと、5回ログに表示されます
        do dbg@print("初期値が+1されました\n")
    end for
    
    ; 増減値を2にしてみました
    ; for(初期値, 終値, 増減値)
    for(1, 5, 2)
        ; 初期値が+2されましたと、3回ログに表示されます
        do dbg@print("初期値が+2されました\n")
    end for
end func
